CREATE TABLE PART 'Parts' 1
PARTNAME (CHAR,15,'Part Number')
PART (INT,13,'Part (ID)')
TYPE (CHAR,1,'Type (P/R/O)')
PROC (INT,13,'Routing (ID)')
PARTDES (CHAR,48,'Part Description')
UNIT (INT,13,'Factory Unit (ID)')
UPD (CHAR,1,'Upd SU & Proc Param')
LEADTIME (INT,8,'Min. Lead Time')
STATUS (CHAR,1,'Out of Use/Base Prod')
FATQUANT (REAL,16,3,'Qty(Parent)')
PRICE (REAL,16,2,'Std Cost')
LASTPRICE (REAL,13,2,'Last Price')
COST (REAL,16,2,'Cost')
REV (INT,13,'Revision (ID)')
PRIVTYPE (CHAR,1,'Quality Code (Part)')
PATTERN (INT,13,'Template (ID)')
SERNPATTERN (INT,13,'Serial No. Templ(ID)')
PUNIT (INT,13,'Unit (ID)')
CONV (REAL,14,'Conversion Ratio')
AVGDATE (DATE,8,'Date Ready for Use')
KITFLAG (CHAR,1,'Issued to Kits?')
DOLLAR (REAL,13,3,'Actual Cost {F.F}')
TAKEFLAG (CHAR,1,'Manual Issue?')
WTYPE (CHAR,1,'Not in Use')
PREFERRED (CHAR,1,'Preferred Status')
EXCESS (CHAR,1,'Excess Inv. B/N/C/I')
PARTPARAM (INT,13,'Part Parameters (ID)')
PDES (INT,13,'Part Description(ID)')
COSTDATE (DATE,8,'Cost Date')
MPART (INT,13,'MPART (ID)')
AUTOSERIAL (CHAR,1,'Open Automatically?')
SIZEBAR (INT,13,'Size Chart (ID)')
CURRENCY (INT,13,'Currency (ID)')
SCRAP (REAL,10,2,'Scrap')
SECONDCOST (REAL,16,2,'Cost {F.F}')
SECONDPRICE (REAL,16,2,'Std Cost {F.F}')
COSTQUANT (REAL,9,3,'Costing Unit')
ACTUALCOSTTYPE (CHAR,1,'Costing Method')
SERNFLAG (CHAR,1,'Maintain Serial Nos?')
FAMILY (INT,13,'Family (ID)')
CURDATE (DATE,8,'Date')
LASTCURRENCY (INT,13,'Last Price Curr (ID)')
LASTCOSTFLAG (CHAR,1,'Price Source')
REVFLAG (CHAR,1,'Revisions?')
PRICEFLAG (CHAR,1,'Source of Std Cost')
FOBPRICE (REAL,13,2,'Commission Price')
FOBCURRENCY (INT,13,'Curr')
PURPRICE (REAL,13,2,'Purchase Price')
USER (INT,13,'User (ID)')
UDATE (DATE,14,'Time Stamp')
BARCODE (CHAR,16,'Bar Code')
MAXREPAIRPRICE (REAL,13,2,'Maximum Repair Price')
REPAIRCURRENCY (INT,13,'Repair Currency (ID)')
UNSPSC (INT,13,'UN/SPSC Code (ID)')
MOLDPART (INT,13,'Byproduct Part (ID)')
RBALANCE (REAL,13,2,'Current Balance')
PARTTYPE (INT,13,'Product Type')
PARTSTAT (INT,13,'Part Status (ID)')
OWNER (INT,13,'Assigned to (ID)')
EXTFILEFLAG (CHAR,1,'Attachments?')
LOCFLAG (CHAR,1,'Designation Control?')
PRICEPOLICY (CHAR,2,'Price Policy')
TURNKEY (CHAR,1,'Turnkey')
SHOWINWEB (CHAR,1,'Purch\'d via Web Site')
PALLETTYPE (INT,13,'Pallet Type (ID)')
SUPERPHARM (INT,2,'Superpharm Attribute')
NOTFIXEDCONV (CHAR,1,'Variable Conversion')
LOTBYVENDOR (CHAR,1,'Internal/Vendor Lot#')
MINPRICE (REAL,13,2,'Minimum Price')
STORAGETYPE (INT,13,'Storage Group (ID)')
AUTOUNIQUE (PART)
UNIQUE (PARTNAME)
NONUNIQUE (PROC)
NONUNIQUE (PARTDES)
NONUNIQUE (FAMILY)
NONUNIQUE (BARCODE)
NONUNIQUE (UNSPSC)
NONUNIQUE (MOLDPART)
NONUNIQUE (MPART)
NONUNIQUE (PARTSTAT)
;

